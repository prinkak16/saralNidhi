  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "ng build --optimization=false && electron .",
    "electron-build-prod-run": "cross-env NODE_ENV=prod node configuration && ng build --optimization=false --prod && electron .",
    "electron-build-staging-run": "cross-env NODE_ENV=staging node configuration && ng build --optimization=false --prod -c=staging && electron .",
    "build:installer": "node build.js",
    "pack:win": "./node_modules/.bin/electron-packager . SaralNidhi --overwrite --asar --icon=nidhi_logo.ico --prune --ignore=webpack.config.js --ignore=^/tmp --ignore=^/.git --out=builds --platform=win32 --arch=x64 --version-string.CompanyName='Jarvis Consulting' --version-string.ProductName='Saral Nidhi Application'",
    "electron-build-prod": "cross-env NODE_ENV=prod node configuration && ng build --optimization=false --prod && npm run pack:win && npm run build:installer",
    "electron-build-staging": "cross-env NODE_ENV=staging node configuration && ng build --optimization=false --prod -c=staging && npm run pack:win && npm run build:installer",
    
    
    
    "build:staging": "env-cmd -f .env.staging npm run build-staging"
  },
